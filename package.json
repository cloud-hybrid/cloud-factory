{
    "name": "cloud-factory",
    "private": false,
    "version": "0.4.89",
    "type": "module",
    "types": "index.d.ts",
    "main": "index.js",
    "description": "A CLI Utility to Manage Software-Defined State",
    "repository": "git+https://github.com/cloud-hybrid/factory.git",
    "license": "BSD-2-Clause",
    "author": "Jacob B. Sanders",
    "scripts": {
        "get": "cdktf get",
        "start": "ts-node listener.js start",
        "build": "tsc --pretty",
        "compile": "tsc --watch",
        "test": "node --no-warnings --es-module-specifier-resolution node ./src/testing",
        "test-resources": "node --no-warnings --es-module-specifier-resolution node ./resources/constructs/ssm-parameter",
        "setup": "cdktf get && tsc",
        "upload": "npm publish --access public",
        "reinstall": "npm update && npm install --global --force",
        "prepublishOnly": "node --no-warnings --es-module-specifier-resolution node ci-cd",
        "constructs": "cdktf get --language typescript",
        "upgrade": "npm i cdktf@latest cdktf-cli@latest",
        "upgrade-next": "npm i cdktf@next cdktf-cli@next",
        "rm-declarations": "find . -name '*.d.ts' -type f -delete",
        "rm-declaration-maps": "find . -name '*.d.ts.map' -type f -delete",
        "rm-maps": "find . -name '*.js.map' -type f -delete",
        "rm-binaries": "find . -name '.terraform' -type d -delete"
    },
    "dependencies": {
        "@aws-sdk/client-s3": "^3.45.0",
        "@aws-sdk/client-secrets-manager": "^3.46.0",
        "@aws-sdk/credential-providers": "^3.46.0",
        "@cdktf/provider-archive": "^0.3.14",
        "@cdktf/provider-aws": "^5.0.12",
        "@cloud-technology/parameter": "^0.4.125",
        "@cloud-technology/secrets-manager-client": "^0.2.14",
        "@internal/ci-cd": "./ci-cd",
        "ajv": "^8.8.2",
        "ajv-formats": "^2.1.1",
        "cdktf": "latest",
        "cdktf-cli": "latest",
        "chalk": "^5.0.0",
        "constructs": "^10.0.31",
        "dotenv": "^16.0.0",
        "inquirer": "^8.2.0",
        "jsonschema": "^1.4.0",
        "source-map-support": "latest",
        "ts-node": "latest",
        "uuid": "^8.3.2",
        "yargs": "^17.3.0"
    },
    "devDependencies": {
        "@aws-cdk/assert": "latest",
        "@aws-sdk/types": "latest",
        "@swc/core": "^1.2.138",
        "@swc/helpers": "^0.3.2",
        "@types/follow-redirects": "^1.14.0",
        "@types/inquirer": "latest",
        "@types/json-schema": "latest",
        "@types/node": "latest",
        "@types/nodemon": "^1.19.1",
        "@types/source-map-support": "latest",
        "@types/uuid": "latest",
        "@types/yargs": "latest",
        "@typescript-eslint/eslint-plugin": "latest",
        "@typescript-eslint/parser": "latest",
        "@typescript-eslint/utils": "latest",
        "eslint": "latest",
        "nodemon": "^2.0.15",
        "regenerator-runtime": "^0.13.9",
        "ts-jest": "latest",
        "ts-node": "^10.5.0",
        "tsconfig-paths": "^3.12.0",
        "tslib": "^2.3.1",
        "typescript": "latest",
        "typescript-eslint-language-service": "latest"
    },
    "exports": {
        ".": "./src/cli/index.js"
    },
    "eslintConfig": {
        "parser": "@typescript-eslint/parser",
        "plugins": [
            "@typescript-eslint"
        ],
        "extends": [
            "eslint:recommended",
            "plugin:@typescript-eslint/eslint-recommended",
            "plugin:@typescript-eslint/recommended"
        ],
        "rules": {
            "no-case-declarations": 0,
            "@typescript-eslint/no-explicit-any": 0,
            "@typescript-eslint/explicit-function-return-type": 0,
            "@typescript-eslint/no-use-before-define": 0,
            "@typescript-eslint/no-empty-interface": 0,
            "@typescript-esline/no-async-promise-executor": 0,
            "@typescript-eslint/explicit-module-boundary-types": 0,
            "@typescript-eslint/no-empty-function": 0,
            "@typescript-eslint/no-unused-vars": 0,
            "@typescript-eslint/no-inferrable-types": 0,
            "@typescript-eslint/ban-types": 0,
            "@typescript-eslint/ban-ts-comment": 0,
            "no-sequences": "error"
        },
        "ignorePatterns": [
            "ci",
            "dist",
            "coverage",
            "distribution",
            "node_modules"
        ]
    },
    "bin": {
        "cloud-factory": "./index.js"
    }
}
